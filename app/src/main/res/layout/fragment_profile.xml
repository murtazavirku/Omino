                        android:layout_marginRight="2dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/grey_10"
                        android:src="@drawable/ss2"
                        android:scaleType="fitXY"

                        ></ImageView>


                    <LinearLayout
